# API Configuration
EXPO_PUBLIC_API_URL=https://api.mushroomhunter.app
EXPO_PUBLIC_API_VERSION=v1

# AI Service
EXPO_PUBLIC_AI_API_KEY=your_ai_api_key_here
EXPO_PUBLIC_AI_MODEL_VERSION=mushroom-classifier-v2

# Maps Configuration
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
EXPO_PUBLIC_MAPBOX_TOKEN=your_mapbox_token_here

# Storage
EXPO_PUBLIC_AWS_S3_BUCKET=mushroom-hunter-uploads
EXPO_PUBLIC_AWS_REGION=eu-west-1

# Analytics (optional)
EXPO_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_token_here
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
EXPO_PUBLIC_ENABLE_AR_MODE=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_SOCIAL_SHARING=true

# Environment
EXPO_PUBLIC_ENVIRONMENT=development